            this.healing = new Skill(Integer.parseInt(listIn[26]));
            this.taunting = new Skill(Integer.parseInt(listIn[27]));
            this.tracking = new Skill(Integer.parseInt(listIn[28]));
            this.charm = new Skill(Integer.parseInt(listIn[29]));

